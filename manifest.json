{
  "manifest_version": 2,

  "name": "Gc Notes",
  "description": "Add personal notes on every internet pages",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_title": "Gc Notes"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ]
}